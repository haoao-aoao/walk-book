<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.neusoft.demo.activemq.dao.ActiveMQDao">

    <!--新增数据-->
    <insert id="addActiveMQ" parameterType="String">
        insert into t_activemq
            (code,
             info,
             creat_time)
             values
             (#{param1},
              #{param2},
              now())
    </insert>
</mapper>